type ToDoContent is {
  itemId is Id(ToDoList.ToDoItem),
  itemDescription is String,
  expectedCompletion is TimeStamp?
}

type ToDoItemInput is {
  content is ToDoContent
}

context ToDoListUI is {
  // UI elements using valid group syntax
  page MainPage is {
    list ListOfToDoItems shows type ToDoContent
    form EnterToDoItem acquires type ToDoItemInput
    button MarkItemDone activates type ToDoContent
  }
} with {
  option is technology("react.js")
}
